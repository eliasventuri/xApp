{
  "name": "StormXApp",
  "version": "0.2.1",
  "description": "Xisquio desktop app based in peerflix module, packaged with node-webkit",
  "main": "app://stormx/app/views/index.html",
  "window": {
    "title": "StormX App",
    "toolbar": false,
    "frame": false,
    "width": 1146,
    "height": 645,
    "min_width": 1146,
    "min_height": 645,
    "resizable": true,
    "show": false,
    "icon": "app/assets/icons/512x512.png"
  },
  "scripts": {
    "install": "gulp install",
    "start": "nodewebkit . -- --debug",
    "build": "gulp build",
    "watcher": "gulp watch"
  },
  "dependencies": {
    "eztv": "0.0.3",
    "i18n": "~0.4.1",
    "magnet-uri": "~1.1.0",
    "moment": "~2.5.1",
    "network-address": "0.0.4",
    "numeral": "^1.5.3",
    "peerflix": "~0.8.3",
    "read-torrent": "~1.0.0",
    "request": "~2.34.0"
  },
  "devDependencies": {
    "gulp": "~3.5.6",
    "gulp-inject": "~0.4.1",
    "gulp-clean": "~0.2.4",
    "gulp-concat": "~2.2.0",
    "gulp-less": "~1.2.3",
    "gulp-zip": "^0.3.4",
    "nodewebkit": "0.9.2-4",
    "node-webkit-builder": "~0.0.2"
  }
}
